<%= form_for @bookmark do |f| %>
    <%= f.label :title %> <br>
    <%= f.text_field :title %> <br>
    <%= f.label :url %> <br>
    <%= f.field :url %> <br>
    <%= f.label :comment %> <br>
    <%= f.text_area :comment %> <br>
    <%= f.label :category %> <br>
    <%= f.radio_button :category, "funny", "useful", "serious" %>
    <%= f.label :is_checked_favorite %> <br>
    <%= f.check_box :is_checked_favorite, "favorite?" %> <br>
    <%= f.submit %>
    <% end %>

