<h3>Manage Your Bookmarks</h3>

<ul>

  <% @bookmarks.each do |bookmark| %>
  <li> <%= link_to(bookmark.title, bookmark.url, :target => "_blank") %>
      | <%= link_to('Edit', bookmark) %>
      | <%= link_to('Delete', bookmark, method: :delete, data: {confirm: 'Are you sure?'})%>
  </li>

  <% end %>

</ul>

<h4> <%= link_to('Back to My Bookmarks', bookmarks_path) %> </h4>
