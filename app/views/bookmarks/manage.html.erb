<div class="page-header">
  <h1>Manage Your Bookmarks <small>edit or delete</small></h1>
</div>

<ul>

  <% @bookmarks.each do |bookmark| %>
  <li style="list-style-type: none;"> <%= link_to(bookmark.title, bookmark.url, :target => "_blank") %>
      | <%= link_to('Edit', bookmark) %>
      | <%= link_to('Delete', bookmark, method: :delete, data: {confirm: 'Are you sure?'})%>
  </li>

  <% end %>

</ul>

<h4> <%= link_to('Back to My Bookmarks', bookmarks_path) %> </h4>
